@model TimesheetWeb.ViewOutput

<h1>Timesheet: Estimated Hours</h1>
<h2>Month: @Model.month</h2>
<p>Hours</p>
<table>
    <tr>
        <td>Date</td><td>Start</td><td>End</td>
    </tr>
    @foreach (var hour in Model.hours)
    {
        <tr>
            <td>@hour.date</td><td>@hour.start</td><td>@hour.end</td>
        </tr>
    }
</table>
